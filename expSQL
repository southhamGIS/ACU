
--EXPERIMENT SQL
--LEVEL1 NATION
SELECT * FROM SAMPLETABLE   WHERE BITAND(UNITID，NATION_UNITID_MAX)=NATION_UNITID  AND ROWNUM < 1001

    ----  contrast experiment   
SELECT *
  FROM SAMPLETABLE 
 WHERE UNITID IN
       (SELECT UNITID
          FROM UNITTABLE
         WHERE PARANTID IN
               (SELECT UNITID
                  FROM SAMPLETABLE 
                 WHERE A.PARANTID IN
                       (SELECT UNITID
                          FROM SAMPLETABLE 
                         WHERE PARANTID = NATION_UNITID)))

-----LEVEL2 PROVINCE
  
 SELECT * FROM SAMPLETABLE  WHERE BITAND(UNITID，PROVINCE_UNITID_MAX)=PROVINCE_UNITID 

    ----  contrast experiment   

SELECT *
  FROM SAMPLETABLE 
 WHERE UNITID IN
       (SELECT UNITID
          FROM UNITTABLE
         WHERE PARANTID IN ((SELECT UNITID
                              FROM UNITTABLE A
                             WHERE A.PARANTID = PROVINCE_UNITID)))  



SELECT * FROM SAMPLETABLE  WHERE BITAND(UNITID，PROVINCE_UNITID_MAX)=PROVINCE_UNITID 

----  contrast experiment   

SELECT *
  FROM SAMPLETABLE
 WHERE UNITID IN
       (SELECT UNITID
          FROM UNITTABLE
         WHERE PARANTID IN ((SELECT UNITID
                              FROM UNITTABLE A
                             WHERE A.PARANTID = PROVINCE_UNITID)))

-----LEVEL3 CITY

SELECT * FROM SAMPLETABLE  WHERE BITAND(UNITID，CITY_UNITID_MAX)=CITY_UNITID 

----  contrast experiment   


SELECT *
  FROM SAMPLETABLE Z
 WHERE UNITID IN (SELECT UNITID
                      FROM UNITTABLE A
                     WHERE A.PARANTID = CITY_UNITID)
                     


 SELECT * FROM SAMPLETABLE  WHERE BITAND(UNITID，CITY_UNITID_MAX)=CITY_UNITID 
     
SELECT *
  FROM SAMPLETABLE
 WHERE UNITID IN (SELECT UNITID
                      FROM UNITTABLE A
                     WHERE A.PARANTID = CITY_UNITID)  

   SELECT * FROM SAMPLETABLE  WHERE BITAND(UNITID，CITY_UNITID_MAX)=CITY_UNITID 
    
SELECT *
  FROM SAMPLETABLE
 WHERE UNITID IN (SELECT UNITID
                      FROM UNITTABLE A
                     WHERE A.PARANTID = 278107521024)  
                     

   SELECT * FROM SAMPLETABLE  WHERE BITAND(UNITID，CITY_UNITID_MAX)=CITY_UNITID 
    ---
SELECT *
  FROM SAMPLETABLE
 WHERE UNITID IN (SELECT UNITID
                      FROM UNITTABLE A
                     WHERE A.PARANTID = CITY_UNITID) 


--LEVEL4 COUNTRY
   
SELECT * FROM SAMPLETABLE WHERE UNITID =COUNTRY_UNITID


